{
  "name": "room-homepage",
  "version": "1.0.0",
  "description": "FrontendMentor.io Project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass:watch": "sass --watch styles/scss/index.scss:styles/css/index.css --style=expanded --load-path=styles/scss",
    "serve": "browser-sync start --server --files 'index.html, styles/css/*.css'",
    "dev": "concurrently -k -n SASS,BS \"npm run sass:watch\" \"npm run serve\"",
    "build": "sass styles/scss/index.scss:styles/css/index.css --style=compressed --no-source-map --load-path=styles/scss",
    "deploy": "gh-pages -d ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edsonv/FrontendMentor.io---Room-Homepage.git"
  },
  "author": "Edson J. Vargas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edsonv/FrontendMentor.io---Room-Homepage/issues"
  },
  "homepage": "https://github.com/edsonv/FrontendMentor.io---Room-Homepage#readme",
  "devDependencies": {
    "browser-sync": "^3.0.4",
    "concurrently": "^9.2.1",
    "gh-pages": "^6.3.0",
    "sass": "^1.91.0"
  }
}
